{
  "name": "log-to-csv",
  "version": "1.0.0",
  "description": "log-to-csv",
  "main": "./dist/index.js",
  "typings": "./Interfaces/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "7.0.4",
    "ts-loader": "1.3.3",
    "typescript": "2.1.5",
    "webpack": "1.14.0"
  },
  "files": [
    "dist/",
    "Interfaces/index.d.ts"
  ],
  "scripts": {
    "build": "npm run build:umd && npm run build:umd:debug",
    "build:umd": "webpack ./src/index.ts ./dist/index.js --output-library-target=\"umd\" -p --config webpack.production.config.js",
    "build:umd:debug": "webpack ./src/index.ts ./dist/index.debug.js --output-library-target=\"umd\" -d",
    "prepublish": "npm run build"
  },
  "author": "Islam Attrash <isattrash@gmail.com>",
  "license": "MIT"
}
